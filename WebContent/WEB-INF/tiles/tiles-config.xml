<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<!-- tile register -->
	<definition name="t_base" template="/WEB-INF/views/templates/template.jsp">
		<put-attribute name="title" value="team"/>
		<put-attribute name="header" value="/WEB-INF/views/templates/header.jsp"/>
		<put-attribute name="nav" value="/WEB-INF/views/templates/nav.jsp"/>
		<put-attribute name="remote" value="/WEB-INF/views/remote/recentView.jsp"/>
		<put-attribute name="footer" value="/WEB-INF/views/templates/footer.jsp"/>
	</definition>
	<definition name="t_expr" extends="t_base">
		<put-attribute name="section" expression="/WEB-INF/views/${section}.jsp"/>
	</definition>
	<definition name="t_notice" extends="t_base">
		<put-attribute name="title" value="notice"/>
		<put-attribute name="section" expression="/WEB-INF/views/${section}.jsp"/>
	</definition>
	<definition name="t_QnA" extends="t_base">
		<put-attribute name="title" value="QnA"/>
		<put-attribute name="section" expression="/WEB-INF/views/${section}.jsp"/>
	</definition>
	<definition name="t_event" extends="t_base">
		<put-attribute name="title" value="EVENT"/>
		<put-attribute name="section" expression="/WEB-INF/views/${section}.jsp"/>
	</definition>
	<definition name="t_inquire" extends="t_base">
		<put-attribute name="title" value="inquire"/>
		<put-attribute name="section" expression="/WEB-INF/views/${section}.jsp"/>
	</definition>
	<definition name="t_return" extends="t_base">
		<put-attribute name="title" value="return"/>
		<put-attribute name="section" expression="/WEB-INF/views/${section}.jsp"/>
	</definition>
	
</tiles-definitions>



